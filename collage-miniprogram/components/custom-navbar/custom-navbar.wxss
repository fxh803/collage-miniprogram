.navbar {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100vw;
  position: relative;
  background-color: white;
}
.navbar .button{
  position: absolute;
  left: 0px;
  width: 40px;
  height: 40px;
  margin: 0px;
  margin-left: 10px;
  padding: 0;
  background-color: white;
}
.btn_icon{
  position: absolute;
  top:50%;
  transform: translate(-50%,-50%);
  left: 50%;
  width: 20px;
  height: 20px;
  z-index: 999;
}
.text{
  z-index: 999;
  font-size: 14px;
  left:50%;
}


